		function getPerticularEvent(id) {
			$.ajax({
				type : "POST",
				contentType : "application/json",
				url : "/getEventDetails/"+id,
				data : "",
				dataType : 'json',
				timeout : 100000,
				success : function(data) {
					$('#modelBody').html(data.description);
					$('#modalHeader').html(data.name);
					$('#myModal2').modal('show'); 
				},
				error : function(e) {
				},
				done : function(e) {
				}
			});
		}
	</script>
